<Window x:Class="Diplom2.SetImageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:Diplom2"
        mc:Ignorable="d"
        Title="SetLabelWindow" Height="250" Width="300">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Label Content="Ширина"/>

            <TextBox Name="textBoxWidth"/>
            <Label Content="Высота"/>
            <TextBox Name="textBoxHeight"/>
            <Label Content="Выбрать путь"/>
            <TextBox Name="textBoxPath" />
            <Button Content="Выбрать" Name="buttonPath" Click="ChoosePath"/>

            <Button Content="Удалить" Name="buttonRemove" Click="ClickRemove"></Button>
            <Button Content="Принять" Name="buttonSubmit" Click="ClickSubmit"></Button>
        </StackPanel>
    </Grid>
</Window>
