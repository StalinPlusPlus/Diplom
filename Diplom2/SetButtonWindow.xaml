<Window x:Class="Diplom2.SetButtonWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:Diplom2"
        mc:Ignorable="d"
        Title="SetButtonWindow" Height="250" Width="300">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Label Content="Ширина"/>
            <TextBox Name="textBoxHeight"/>

            <Label Content="Высота"/>
            <TextBox Name="textBoxWidth"/>

            <Label Content="Текст"/>
            <TextBox Name="textBoxText"/>

            <Button Content="Удалить" Name="buttonRemove" Click="ClickRemove"></Button>
            <Button Content="Принять" Name="buttonSubmit" Click="ClickSubmit"></Button>
        </StackPanel>
    </Grid>
</Window>
